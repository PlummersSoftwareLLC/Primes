10 REM BASED ON REMARKABLE BASIC SIEVE BY DAVEPL
20 DIM A%(4998)
30 N = 10000
40 NHALF% = INT((N - 3) / 2)
50 NSQ% = INT((-1 + SQR(1 + 2 * NHALF%)) / 2)
60 FOR I = 1 TO NSQ%
70     IF A%(I) = 1 THEN GOTO 110
80     FOR J = 2 * INT(I * (I + 1)) TO NHALF% STEP I * 2 + 1
90        A%(J) = 1
100    NEXT J
110 NEXT I
120 END
